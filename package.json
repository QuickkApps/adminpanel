{
  "name": "anume-admin-panel",
  "version": "1.0.0",
  "description": "Admin panel for remotely managing Anume app configuration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run setup-production",
    "test": "jest",
    "postinstall": "npm run setup-production",
